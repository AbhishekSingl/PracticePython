    "def fib(n):\n",
    "    f=list()\n",
    "    i=0\n",
    "    y1=1\n",
    "    f = [0,1]\n",
    "    while(i<n-2):\n",
    "        if i==0:\n",
    "            y3=y2=y1+i\n",
    "            \n",
    "        else:\n",
    "            y3=y1+y2\n",
    "            y1=y2\n",
    "            y2=y3\n",
    "        f.append(y3)\n",
    "        \n",
    "        \n",
    "        i+=1\n",
    "    return f"
   ]
